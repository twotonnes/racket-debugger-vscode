{
  "name": "racket-debugger",
  "displayName": "Racket Debugger",
  "description": "Adds debugger support for Racket.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onDebug",
    "onLanguage:racket"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "debuggers": [{
      "type": "racket",
      "label": "Racket Debug",
      "languages": ["racket"],
      "configurationAttributes": {
        "launch": {
          "required": ["program", "debugServer"],
          "properties": {
            "program": {
              "type": "string",
              "description": "Absolute path to a Racket source file.",
              "default": "${file}"
            },
            "debugServer": {
              "type": "number",
              "description": "Debug server port to connect to.",
              "default": 4142
            }
          }
        }
      },
      "initialConfigurations": [
        {
          "type": "racket",
          "request": "launch",
          "name": "Launch Racket Program",
          "program": "${file}",
          "debugServer": 4142
        }
      ]
    }],
    "commands": [
      {
        "command": "racket-debugger.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "esbuild": "^0.25.10",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
